    "it-length-prefixed": "^3.0.0",
    "it-pb-rpc": "^0.1.5",
    "it-pipe": "^1.1.0",
    "libp2p": "^0.27.3",
    "libp2p-crypto": "~0.16.3",
    "libp2p-mplex": "^0.9.3",
