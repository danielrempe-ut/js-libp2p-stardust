{
  "name": "libp2p-stardust",
  "version": "0.1.0",
  "description": "Basically ws-star but without the bugs",
  "main": "src/index.js",
  "bin": {
    "stardust-server": "src/server/bin.js"
  },
  "scripts": {
    "lint": "aegir lint",
    "build": "aegir build",
    "test": "aegir test",
    "test:node": "aegir test --target node",
    "test:browser": "aegir test --target browser",
    "test:webworker": "aegir test --target webworker",
    "release": "aegir release --target node --target browser",
    "release-minor": "aegir release --type minor --target node --target browser",
    "release-major": "aegir release --type major --target node --target browser",
    "coverage": "aegir coverage",
    "coverage-publish": "aegir coverage --provider coveralls"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "ws-star",
    "rendezvous",
    "relay"
  ],
  "author": "Maciej Krüger <mkg20001@gmail.com>",
  "license": "MPL-2.0",
  "dependencies": {
    "libp2p-crypto": "~0.16.3",
    "libp2p-mplex": "~0.8.5",
    "libp2p-websockets": "~0.12.4",
    "mafmt": "^7.0.0",
    "multiaddr": "^7.1.0",
    "multistream-select": "~0.14.6",
    "peer-id": "~0.12.5",
    "peer-info": "~0.15.1",
    "protons": "^1.0.1"
  },
  "devDependencies": {
    "aegir": "^20.4.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p-stardust.git"
  },
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-stardust/issues"
  },
  "homepage": "https://github.com/libp2p/js-libp2p-stardust#readme",
  "contributors": [
    "Jacob Heun <jacobheun@gmail.com>",
    "Maciej Krüger <mkg20001@gmail.com>"
  ]
}
